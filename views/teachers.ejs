<%- include('./partials/head.ejs', {namePage: 'Teacher'}); %>
  <table>
    <thead>
      <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% teachers.forEach(teacher => { %>
      <tr>
        <td><%= teacher.id%></td>
        <td><%= teacher.first_name%></td>
        <td><%= teacher.last_name%></td>
        <td><%= teacher.email%></td>
        <td><%= checkNull(teacher) %></td>
        <td><a href="/teachers/edit/<%=teacher.id%>">Edit</a></td>
        <td><a href="/teachers/delete/<%=teacher.id%>" onclick="return alert('Data deleted!')">Delete</a></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
  <br>
  </body>
</html>